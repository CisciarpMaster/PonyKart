material ribbon {
	technique {
		pass {
			lighting off
			scene_blend alpha_blend
			depth_write off

			texture_unit {
				texture ribbonband.png 1d
				tex_address_mode clamp
				filtering none
			}
		}
	}
}

material antibody
{
	technique
	{
		pass
		{
			ambient 0.588235 0.588235 0.588235 1
			diffuse 0.588235 0.588235 0.588235 1
			specular 0 0 0 10 0
		}

	}

}