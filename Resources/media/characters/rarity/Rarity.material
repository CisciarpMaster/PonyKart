material RarityBody {
    technique {
		pass {
			vertex_program_ref VertEdge {
			}
			fragment_program_ref outlined_ps {
				param_named OutlineColour float3 0.749 0.749 0.7647
			}
		}

        pass {
			vertex_program_ref fresnelTex_vp {
			}
			fragment_program_ref fresnelTex_Self_fp {
				param_named Alpha float 0.2
			}
			
            texture_unit {
                texture "RarityBody.dds"
				mipmap_bias -1
            }
        }
    }
}

