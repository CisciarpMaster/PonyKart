material AppleCartExtras
{
	receive_shadows off
	
	technique
	{
		pass
		{
			scene_blend alpha_blend
			cull_hardware none
			depth_write off
			
			texture_unit
			{
				tex_address_mode clamp
				texture applecart_tex.dds
			}
		}
	}
}

material AppleCart
{
	technique
	{
		pass
		{
			texture_unit
			{
				texture applecart_tex.dds
			}
		}
	}
}