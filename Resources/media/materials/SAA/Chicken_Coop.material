material Chicken_coop_sign
{
	receive_shadows off
	
	technique
	{
		pass
		{
			scene_blend alpha_blend
			depth_write off
			transparent_sorting on
			
			texture_unit
			{
				texture chickencoopsign.dds
				tex_address_mode clamp
			}
		}
	}
}

material Chicken_Coop
{
	technique
	{
		pass
		{
			cull_hardware none
			
			texture_unit
			{
				texture chickencoop.dds
			}
		}
	}
}

