material SAA_BannerSign {
	receive_shadows off
	
	technique {
		pass {
			vertex_program_ref banner_vp {
				param_named scale float2 1 1
			}

			fragment_program_ref fresnelTex_Self_fp {
			}

			scene_blend alpha_blend
			depth_write off
			alpha_rejection greater_equal 64
			
			texture_unit {
				texture "banner.dds"
			}
		}
	}
}

