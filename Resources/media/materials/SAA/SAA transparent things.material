material Billboards {
	receive_shadows off
	
	technique {
		pass {
			transparent_sorting on
			scene_blend alpha_blend
			depth_write off
			alpha_rejection greater_equal 64

			texture_unit {
				texture "billboards.dds"
				tex_address_mode clamp
			}
		}
	}
}

material AppleTreeBillboards {
	receive_shadows off
	
	technique {
		pass {
			transparent_sorting on
			scene_blend alpha_blend
			//depth_write off
			alpha_rejection greater_equal 64
			lighting off

			texture_unit {
				texture "apple tree billboards.dds"
				tex_address_mode clamp
			}
		}
	}
}

