material SAA_CornRows {
	receive_shadows off
	
	technique {
		pass {

			vertex_program_ref corn_vp {
				param_named scale float2 0.033333 1
			}

			//fragment_program_ref fresnelTex_Self_fp {
			//}

			scene_blend alpha_blend
			depth_write off
			
			texture_unit {
