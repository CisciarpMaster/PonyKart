
material zerg
{
	receive_shadows off
	
	technique
	{
		pass
		{
			ambient 1 1 1 1
			diffuse 1 1 1 1
			specular 0 0 0 0 0

			texture_unit
			{
				texture zerg.tga
			}
		}
		
		pass {
			scene_blend add
			depth_check on
			lighting on
			
			vertex_program_ref glowBalloon_vp {
			}
			fragment_program_ref glowBalloon_fp {
				param_named GlowColor float3 1 1 1
				param_named GlowExpon float  2.5
			}
		}
	}

}


