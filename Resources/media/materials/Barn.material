material Barn
{
	receive_shadows off
	
	technique
	{
		pass
		{
			cull_hardware none
			
			texture_unit
			{
				texture saa_barn.dds
			}
		}
	}
}

material Barn_Plants
{
	receive_shadows off
	
	technique
	{
		pass
		{
			scene_blend alpha_blend
			depth_write off
			cull_hardware none
			transparent_sorting on

			texture_unit
			{
				texture saa_barn_transparent.dds
				tex_address_mode clamp
			}
		}
	}
}