material saa_sky {
	receive_shadows off
	
	technique {
		pass {
			lighting off
			depth_write off
			fog_override true none
			
			texture_unit {
				// front (-Z), back (+Z), left (-X?), right (+X?), up (+Y), down (-Y)
				cubic_texture "skyboxfront.png" "skyboxback.png" "skyboxleft.png" "skyboxright.png" "skyboxup.png" "skyboxdown.png" separateUV
				tex_address_mode clamp
				mipmap_bias -5
			}
		}
	}
}